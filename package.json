    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/eslint-plugin": "^9.1.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
