{
  "$schema": "https://biomejs.dev/schemas/2.0.5/schema.json",
  "extends": [
    "ultracite"
  ],
  "files": {
    "includes": [
      "**",
      "!src/routeTree.gen.ts"
    ]
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noConsole": {
          "options": {
            "allow": [
              "assert",
              "error",
              "info",
              "warn"
            ]
          }
        }
      }
    }
  }
}